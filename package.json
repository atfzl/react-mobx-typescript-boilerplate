{
  "name": "typescript-react-mobx-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend boilerplate with React, MobX and Typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --colors --port 3000 --open",
    "build": "rm -rf dist && webpack -p --progress --colors",
    "analyze": "webpack -p -- --analyze",
    "lint": "tslint src/**/*.{ts,tsx}",
    "format": "prettier --single-quote --trailing-comma=all --parser=typescript src/**/*.{ts,tsx} --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "@storybook/react": "^3.1.8",
    "@types/classnames": "^2.2.0",
    "@types/node": "^8.0.7",
    "@types/ramda": "^0.24.4",
    "@types/react": "^15.0.34",
    "@types/react-dom": "^15.5.1",
    "@types/react-router": "^4.0.12",
    "@types/webpack": "^3.0.1",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "mobx-react-devtools": "^4.2.15",
    "postcss": "^6.0.4",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "postcss-url": "^7.0.0",
    "prettier": "^1.5.2",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.18.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0"
