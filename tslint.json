{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "member-ordering": [false],
    "ter-indent": [true, 2],
    "quotemark": [
      "single"
    ],
    "arrow-parens": false,
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ]
  }
}
